{
  "name": "simple-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React/Redux boilerplate.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon server",
    "start:dev": "NODE_ENV=development nodemon server --ignore node_modules --ignore src/app",
    "build": "rimraf dist && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint . && stylelint src/app/**/*.css || true",
    "test:unit": "mocha src/server/spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielsiedler/simple-react-redux-boilerplate.git"
  },
  "keywords": [
    "es6",
    "react",
    "redux"
  ],
  "author": {
    "name": "Gabriel Siedler",
    "url": "http://gabriel.siedler.ninja"
  },
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-polyfill": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "file-loader": "^0.9.0",
